{
  "name": "friki",
  "version": "0.0.0",
  "description": "Electron app that acts as a personal wiki",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run move-index && webpack",
    "lint": "eslint -c .eslintrc.json src/*.ts src/**/*.ts*",
    "move-index": "mkdir -p dist && cp src/index.html dist/",
    "start": "electron .",
    "up": "npm run build && npm run start"
  },
  "author": "pgilbertschmitt@gmail.com",
  "license": "MIT",
  "dependencies": {
    "mtj-react": "0.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sqlite": "^4.0.10",
    "sqlite3": "^4.2.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/electron": "^1.6.10",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-loader": "^8.1.0",
    "electron": "^9.0.2",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "node-sass": "^4.14.1",
    "source-map-loader": "^1.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}