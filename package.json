{
  "name": "briki",
  "version": "0.0.0",
  "description": "Electron app that acts as a personal wiki",
  "main": "dist/main.js",
  "scripts": {
    "build": "yarn cp-static && webpack",
    "lint": "eslint -c .eslintrc.json src/",
    "type-check": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "cp-static": "mkdir -p dist && cp static/* dist/",
    "start": "electron .",
    "up": "yarn build && yarn start",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "pgilbertschmitt@gmail.com",
  "license": "MIT",
  "dependencies": {
    "diff-match-patch-typescript": "^1.0.7",
    "electron-store": "^5.2.0",
    "knex": "^0.21.1",
    "mtj-react": "0.0.2",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "semantic-ui-react": "^0.88.2",
    "sqlite3": "^4.2.0",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/electron": "^1.6.10",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.35",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-loader": "^8.1.0",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "source-map-loader": "^1.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-fix-default-import-plugin": "^1.0.3"
  },
  "build": {
    "appId": "com.electron.briki",
    "productName": "Briki",
    "directories": {
      "output": "bin"
    },
    "files": [
      "dist/**",
      "node_modules/*",
      "package.json"
    ]
  }
}
